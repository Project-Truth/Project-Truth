{
    "manifest_version": 2,
    "name": "Project-Truth",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],"background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": {                    
          "32": "32.png"            
        },
        "default_title": "Project-Truth"           
      }
  }